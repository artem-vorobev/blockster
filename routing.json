{
    "error404":{
        "selector":{
            "status":404
        },
        "content":{
            "theme":"default",
            "module":{
                "name":"page",
                "params":{"template":"404.php"}
            }
        }
    },
    "error403":{
        "selector":{
            "status":403
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"page",
                "params":{"template":"auth.php"}
            }
        }
    },
    "ajax":{
        "selector":{
            "uri":"/ajax/{moduleName}",
            "params":{
                "moduleName":{"type":"any"}
            }
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"ajax",
                "params":{"moduleName":"{moduleName}"}
            }
        }
    },
    "adminer":{
        "selector":{
            "uri":"/adminer"
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"admin/adminer",
                "params":{}
            }
        }
    },
    "admin/dashboard":{
        "selector":{
            "uri":"/admin"
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"page",
                "params":{"template":"default.php"}
            },
            "positions":{
                "content":[{"name":"admin/dashboard", "params":{}}]
            }
        }
    },
    "admin/menu":{
        "selector":{
            "uri":"/admin/menu/{action}/{id}",
            "params":{
                "action":{"type":"variants", "variants":["new"], "optional":true, "default":"default"},
                "id":{"type":"numeric", "optional":true}
            }
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"page",
                "params":{"template":"default.php"}
            },
            "positions":{
                "content":[{"name":"admin/menu::{action}", "params":{"id":"{id}"}}]
            }
        }
    },
    "admin/articles":{
        "selector":{
            "uri":"/admin/articles/{action}/{id}",
            "params":{
                "action":{"type":"string", "optional":true, "default":"default"},
                "id":{"type":"numeric"}
            }
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"page",
                "params":{"template":"default.php"}
            },
            "positions":{
                "content":[{"name":"admin/menu::{action}", "params":{"id":"{id}", "parentId":"{id}"}}]
            }
        }
    },
    "articles":{
        "selector":{
            "uri":"{uri}",
            "params":{
                "uri":{"type":"any"}
            }
        },
        "content":{
            "theme":"default",
            "module":{
                "name":"page",
                "params":{"template":"default.php"}
            }
        }
    }
}