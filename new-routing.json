{
    "error-404":{
        "selector":{
            "status":"404"
        },
        "content":{
            "module":{
                "name":"page",
                "params":{"template":"404.php"}
            },
            "theme":"default"
        }
    },
    "error-403":{
        "selector":{
            "status":"403"
        },
        "content":{
            "module":{
                "name":"page",
                "params":{"template":"auth.php"}
            },
            "theme":"admin"
        }
    },
    "ajax":{
        "schema":"{SITE_URL}/ajax/{0}",
        "selector":{
            "uri":"/ajax/(.+)"
        },
        "content":{
            "module":{
                "name":"ajax",
                "params":{"moduleName":"{0}"}
            },
            "theme":"admin"
        }
    },
    "adminer":{
        "schema":"{SITE_URL}/adminer",
        "selector":{
            "uri":"/adminer"
        },
        "content":{
            "module":{
                "name":"admin/adminer",
                "params":{}
            },
            "theme":"admin"
        }
    },
    "admin":{
        "schema":"{SITE_URL}/admin/{0}",
        "selector":{
            "uri":"/admin/?(.*)"
        },
        "content":{
            "module":{
                "name":"page",
                "params":{"template":"default.php"}
            },
            "theme":"admin",
            "positions":{
                "content":[{"name":"admin", "params":{"adminUrl":"{0}"}}]
            }
        }
    },
    "other":{
        "selector":{
            "uri":".+"
        },
        "content":{
            "module":{
                "name":"page",
                "params":{"template":"default.php"}
            },
            "theme":"default"
        }
    }
}