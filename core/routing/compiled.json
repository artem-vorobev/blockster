{
    "error404":{
        "selector":{
            "status":404
        },
        "content":{
            "theme":"default",
            "module":{
                "name":"page",
                "params":{
                    "template":"404.php"
                }
            }
        },
        "name":"error404"
    },
    "error403":{
        "selector":{
            "status":403
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"page",
                "params":{
                    "template":"auth.php"
                }
            }
        },
        "name":"error403"
    },
    "ajax":{
        "selector":{
            "uri":"\/ajax\/{moduleName}",
            "params":{
                "moduleName":{
                    "type":"any"
                }
            },
            "rgxp":"https?:\/\/[^\/]+\/ajax\/(.*)",
            "names":[
                "moduleName"
            ]
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"ajax",
                "params":{
                    "moduleName":"{moduleName}"
                }
            }
        },
        "name":"ajax"
    },
    "adminer":{
        "selector":{
            "uri":"\/adminer",
            "rgxp":"https?:\/\/[^\/]+\/adminer",
            "names":[
                
            ]
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"admin\/adminer",
                "params":[
                    
                ]
            }
        },
        "name":"adminer"
    },
    "admin\/dashboard":{
        "selector":{
            "uri":"\/admin",
            "rgxp":"https?:\/\/[^\/]+\/admin",
            "names":[
                
            ]
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"page",
                "params":{
                    "template":"default.php"
                }
            },
            "positions":{
                "content":[
                    {
                        "name":"admin\/dashboard",
                        "params":[
                            
                        ]
                    }
                ]
            }
        },
        "name":"admin\/dashboard"
    },
    "admin\/menu":{
        "selector":{
            "uri":"\/admin\/menu\/{action}\/{id}",
            "params":{
                "action":{
                    "type":"variants",
                    "variants":[
                        "new"
                    ],
                    "optional":true,
                    "default":"default"
                },
                "id":{
                    "type":"numeric",
                    "optional":true
                }
            },
            "rgxp":"https?:\/\/[^\/]+\/admin\/menu(?:\/(new))?(?:\/([0-9]+))?",
            "names":[
                "action",
                "id"
            ]
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"page",
                "params":{
                    "template":"default.php"
                }
            },
            "positions":{
                "content":[
                    {
                        "name":"admin\/menu::{action}",
                        "params":{
                            "id":"{id}"
                        }
                    }
                ]
            }
        },
        "name":"admin\/menu"
    },
    "admin\/articles":{
        "selector":{
            "uri":"\/admin\/articles\/{action}\/{id}",
            "params":{
                "action":{
                    "type":"string",
                    "optional":true,
                    "default":"default"
                },
                "id":{
                    "type":"numeric"
                }
            },
            "rgxp":"https?:\/\/[^\/]+\/admin\/articles(?:\/([^\/]+))?\/([0-9]+)",
            "names":[
                "action",
                "id"
            ]
        },
        "content":{
            "theme":"admin",
            "module":{
                "name":"page",
                "params":{
                    "template":"default.php"
                }
            },
            "positions":{
                "content":[
                    {
                        "name":"admin\/menu::{action}",
                        "params":{
                            "id":"{id}",
                            "parentId":"{id}"
                        }
                    }
                ]
            }
        },
        "name":"admin\/articles"
    },
    "articles":{
        "selector":{
            "uri":"{uri}",
            "params":{
                "uri":{
                    "type":"any"
                }
            },
            "rgxp":"https?:\/\/[^\/]+(.*)",
            "names":[
                "uri"
            ]
        },
        "content":{
            "theme":"default",
            "module":{
                "name":"page",
                "params":{
                    "template":"default.php"
                }
            }
        },
        "name":"articles"
    }
}